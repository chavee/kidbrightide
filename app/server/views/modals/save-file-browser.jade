.modal-save-file-browser.modal.fade
	.modal-dialog(style="width:50%; margin: auto")
		.modal-content
			.modal-header
				//button.close(data-dismiss='modal') x
				h4
			.modal-body
				input.form-control#file-text(type='text', autofocus)
				hr
				.list-group#file-list
			.modal-footer
				button#btn-cancel(data-dismiss="modal").cancel.btn.btn-default Cancel
				button#btn-ok.btn.btn-primary OK
